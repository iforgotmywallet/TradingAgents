# TradingAgents Environment Configuration
# Copy this file to .env and fill in your API keys

# Required: OpenAI API Key for LLM agents
OPENAI_API_KEY=your_openai_api_key_here

# Required: Finnhub API Key for financial data (free tier available)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Database Configuration for Neon PostgreSQL
NEON_DATABASE_URL=postgresql://username:password@ep-example-123456.us-east-1.aws.neon.tech/neondb?sslmode=require
DB_POOL_SIZE=10
DB_SSL_MODE=require


# Optional: Additional LLM Provider Keys
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GOOGLE_API_KEY=your_google_api_key_here

# Railway Deployment Configuration (automatically set by Railway)
# PORT=8001
# RAILWAY_ENVIRONMENT=production
# RAILWAY_PUBLIC_DOMAIN=your-app.railway.app
# RAILWAY_PROJECT_ID=your-project-id
# RAILWAY_STATIC_URL=https://your-app.railway.app
# RAILWAY_PROJECT_DOMAIN=your-app.railway.app

# Optional: Custom Configuration
# TRADINGAGENTS_RESULTS_DIR=./results
# TRADINGAGENTS_DEBUG=true
# LOG_LEVEL=INFO